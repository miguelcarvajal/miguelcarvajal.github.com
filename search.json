---
layout: null
---
[
  {% for post in site.posts %}
    {
      "name"     : "{{ post.name }}",
      "title"    : "{{ post.title }}",
      "image"    : "{{ post.image }}",
      "url"      : "{{ site.baseurl }}{{ post.url }}",
      "date"     : "{{ post.date }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]